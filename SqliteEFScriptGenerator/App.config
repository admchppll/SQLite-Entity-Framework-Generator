<configuration>
    <configSections>
        <section name="sqliteMappings" type="SqliteEFScriptGenerator.Core.Configuration.SQLiteMappingConfig, SqliteEFScriptGenerator.Core, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" />
    </configSections>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <sqliteMappings>
        <mappings>
            <mapping from="System.String" to="nvarchar" />
            <mapping from="System.Int32" to="integer" />
        </mappings>
    </sqliteMappings>
</configuration>